{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
      "max-len": [2, 150],
      "import/prefer-default-export": [0],
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "react/jsx-one-expression-per-line": [0],
      "react/no-array-index-key": [0],
      "react/no-multi-comp": [0]
    },
    "env": {
      "browser": true,
      "node": true,
      "jasmine": true
    }
}
